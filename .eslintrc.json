{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"indent": ["error", "tab"],
		"linebreak-style": "off",
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"react/prop-types": "off",
		"react/react-in-jsx-scope": "off"
	}
}
